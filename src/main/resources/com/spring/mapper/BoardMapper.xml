<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.spring.mapper.BoardMapper">

<select id="getList" resultType="com.spring.domain.BoardVO">
<![CDATA[
	select * from library_board where bno>0 order by bno
]]>
</select>

<insert id="insertboard">
insert into library_board(bno,title,writer,content,regdate) values(seq_libbno.nextVal,#{title},#{writer},#{content},sysdate)
</insert>
</mapper>