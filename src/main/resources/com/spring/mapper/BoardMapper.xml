<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.spring.mapper.BoardMapper">

<select id="getList" resultType="com.spring.domain.BoardVO">
<![CDATA[
	select * from library_board where bno > 0 order by bno
]]>
</select>

<insert id="insertboard">
		<selectKey keyProperty="bno" order="BEFORE" resultType="int">
			select seq_libbno.nextVal from dual
		</selectKey>
insert into library_board(bno,title,content,writer,regdate)
		values(#{bno},#{title},#{content},#{writer},sysdate)
</insert>

<select id="read" resultType="com.spring.domain.BoardVO">
select bno,title,writer,content,regdate,updatedate from library_board where bno = #{bno}
</select>


</mapper>