<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.spring.mapper.Book_reportMapper">
<insert id="book_reportinsert">
insert into book_report(bno,title,writer,content) values(seq_reportbno.nextVal,#{title},#{writer},#{content})
</insert>

<select id="book_reportList" resultType="com.spring.domain.Book_reportVO">
select * from book_report order by bno
</select>

<select id="book_reportSelectList" resultType="com.spring.domain.Book_reportVO">
select * from book_report where bno=#{bno}
</select>


</mapper>